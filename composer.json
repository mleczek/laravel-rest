{
    "name": "mleczek/laravel-rest",
    "description": "Laravel package with a set of helpful tools for building REST API.",
    "type": "library",
    "keywords": ["laravel", "rest", "api"],
    "homepage": "https://github.com/mleczek/laravel-rest",
    "license": "MIT",
    "authors": [
        {
            "name": "Wojciech Mleczek",
            "email": "mleczek.wojtek@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^5.3",
        "illuminate/http": "^5.3",
        "illuminate/database": "^5.3"
    },
    "require-dev": {
    	"phpunit/phpunit": "~5.7",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Mleczek\\Rest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mleczek\\Rest\\Tests\\": "tests/"
        }
    }
}
