{
    "name": "mleczek/rest",
    "description": "Laravel package with a set of helpful tools for building REST API.",
    "type": "library",
    "keywords": ["laravel", "rest", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Wojciech Mleczek",
            "email": "mleczek.wojtek@gmail.com"
        }
    ],
    "require": {
        "Illuminate/Support": "^5.3",
        "Illuminate/Http": "^5.3",
        "Illuminate/Database": "^5.3"
    },
    "require-dev": {
    	"phpunit/phpunit": "~5.7",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Mleczek\\Rest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mleczek\\Rest\\Tests\\": "tests/"
        }
    }
}
